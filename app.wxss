/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.page {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}
.round-click{
  height: 120rpx;
  width: 120rpx;
  background-color: #ed3f14;
  border-radius: 100%;
  position: fixed;
  bottom: 100rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.round-click navigator{
  font-size: 28rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}
.panelback {
  background-color: #f8f8f9;
}
.iborder {
  border-bottom-style:solid;
  border-width:1rpx;
  border-color:#e9eaec;
}